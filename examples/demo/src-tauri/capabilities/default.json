{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shellx:allow-open",
    "shellx:allow-execute",
    "shellx:allow-spawn",
    "shellx:allow-where-is-command",
    "shellx:allow-kill-pid"
  ]
}